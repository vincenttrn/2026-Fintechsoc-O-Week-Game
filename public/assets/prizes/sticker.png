<svg xmlns="http://www.w3.org/2000/svg" width="200" height="200" viewBox="0 0 200 200">
  <defs>
    <linearGradient id="stickerGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#999;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#666;stop-opacity:1" />
    </linearGradient>
    <filter id="shadow">
      <feDropShadow dx="2" dy="2" stdDeviation="3" flood-opacity="0.5"/>
    </filter>
  </defs>
  <rect width="200" height="200" fill="#1a1a1a" rx="20"/>
  
  <!-- Sticker sheet -->
  <rect x="50" y="40" width="100" height="120" fill="white" stroke="#ddd" stroke-width="2" rx="5" filter="url(#shadow)"/>
  
  <!-- Peel corner -->
  <path d="M 140 50 L 150 40 L 150 50 Z" fill="#f5f5f5" stroke="#ccc" stroke-width="1"/>
  
  <!-- Star sticker 1 -->
  <g transform="translate(75, 70)">
    <polygon points="0,-15 4,-5 15,-5 6,2 10,13 0,6 -10,13 -6,2 -15,-5 -4,-5" 
             fill="#FFD700" stroke="#FFA500" stroke-width="1.5"/>
    <circle cx="0" cy="0" r="3" fill="#FFF" opacity="0.5"/>
  </g>
  
  <!-- Star sticker 2 -->
  <g transform="translate(125, 70)">
    <polygon points="0,-15 4,-5 15,-5 6,2 10,13 0,6 -10,13 -6,2 -15,-5 -4,-5" 
             fill="#4CAF50" stroke="#2E7D32" stroke-width="1.5"/>
    <circle cx="0" cy="0" r="3" fill="#FFF" opacity="0.5"/>
  </g>
  
  <!-- Circle sticker -->
  <circle cx="75" cy="115" r="18" fill="#CA3528" stroke="#8B0000" stroke-width="1.5"/>
  <text x="75" y="121" font-family="Arial" font-size="14" font-weight="bold" 
        text-anchor="middle" fill="white">FS</text>
  
  <!-- Heart sticker -->
  <g transform="translate(125, 115)">
    <path d="M 0,-5 Q -10,-15 -15,-10 Q -18,-5 -15,0 L 0,12 L 15,0 Q 18,-5 15,-10 Q 10,-15 0,-5" 
          fill="#E91E63" stroke="#C2185B" stroke-width="1.5"/>
  </g>
  
  <text x="100" y="195" font-family="Arial, sans-serif" font-size="16" font-weight="bold" 
        text-anchor="middle" fill="#888">STICKER</text>
</svg>
